# OYA AI Workflows - Environment Variables
# Copy this to .env and fill in your values

# OpenAI API (text generation, image generation)
OPENAI_API_KEY=sk-...

# Google Places API (team data scraping)
GOOGLE_PLACES_API_KEY=

# SendGrid (email outreach)
SENDGRID_API_KEY=

# Twilio (SMS outreach)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_PHONE_NUMBER=

# Replicate (video generation)
REPLICATE_API_TOKEN=

# Database (leave empty for SQLite default)
DATABASE_URL=
